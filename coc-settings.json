{
  "emmet.includeLanguages": {
    "liquid": "html",
    "vue-html": "html",
    "vue": "html",
    "svelte": "html"
  },
  "languageserver": {
    "liquid": {
      "command": "theme-check-language-server",
      "filetypes": [
        "liquid"
      ],
      "rootPatterns": [
        ".theme-check.yml"
      ]
    }
  },
  "svelte.enable-ts-plugin": false
}
